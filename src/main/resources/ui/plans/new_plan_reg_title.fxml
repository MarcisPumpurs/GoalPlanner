      <Button fx:id="saveBtn" onAction="#addPlan" layoutX="361.0" layoutY="102.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="74.0" text="Save" />
      <Button fx:id="cancelBtn" onAction="#cancel" layoutX="444.0" layoutY="102.0" mnemonicParsing="false" text="Cancel" />
   </children>
   </AnchorPane>
   </children>
