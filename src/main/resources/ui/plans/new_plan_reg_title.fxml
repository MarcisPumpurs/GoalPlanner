<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="planner.control.plans.PlansAddController">
   <children>
   <AnchorPane id="AnchorPane" fx:id="mainContainer" prefHeight="338.0" prefWidth="464.0" styleClass="custom-pane">
   <children>
      <Label layoutX="73.0" layoutY="68.0" text="New plan title:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="195.0" layoutY="69.0" prefHeight="25.0" prefWidth="241.0" />
      <!-- > <Button layoutX="510.0" layoutY="27.0" mnemonicParsing="false" text="Log out" /> -->
